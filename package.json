{
  "name": "yuking11.net",
  "version": "1.0.0",
  "description": "this project is portfolio-site 'yuking11.net'.",
  "author": "@yuking11",
  "licence": "MIT License",
  "private": true,
  "scripts": {
    "dev": "cross-env NODE_ENV=develop nuxt-ts",
    "build": "cross-env NODE_ENV=production nuxt-ts build",
    "start": "cross-env NODE_ENV=prodcution nuxt-ts start",
    "export": "nuxt-ts export",
    "generate": "yarn build && yarn export",
    "generate:gh-pages": "DEPLOY_ENV=GH_PAGES yarn generate",
    "serve": "nuxt-ts serve",
    "lint:js": "eslint --ext .ts,.js,.vue --ignore-path .gitignore .",
    "lint:style": "stylelint **/*.{vue,scss,css} --ignore-path .gitignore",
    "lint": "yarn lint:js && yarn lint:style",
    "test": "jest"
  },
  "lint-staged": {
    "*.{ts,js,vue}": "eslint",
    "*.{css,vue}": "stylelint"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS",
      "pre-commit": "lint-staged && yarn test"
    }
  },
  "dependencies": {
    "@nuxt/typescript-runtime": "^0.4.10",
    "@nuxtjs/axios": "^5.11.0",
    "cross-env": "^7.0.2",
    "dotenv": "^8.2.0",
    "nuxt": "^2.13.0",
    "nuxt-composition-api": "^0.10.2"
  },
  "devDependencies": {
    "@commitlint/cli": "^9.0.1",
    "@commitlint/config-conventional": "^9.0.1",
    "@nuxt/typescript-build": "^1.0.3",
    "@nuxtjs/eslint-config": "^3.0.0",
    "@nuxtjs/eslint-config-typescript": "^2.0.0",
    "@nuxtjs/eslint-module": "^2.0.0",
    "@nuxtjs/style-resources": "^1.0.0",
    "@nuxtjs/stylelint-module": "^4.0.0",
    "@nuxtjs/svg-sprite": "^0.4.11",
    "@types/fibers": "^3.1.0",
    "@types/lodash": "^4.14.157",
    "@types/sass": "^1.16.0",
    "@vue/test-utils": "^1.0.3",
    "babel-core": "7.0.0-bridge.0",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^26.0.1",
    "eslint": "^7.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-nuxt": "^1.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "fibers": "^5.0.0",
    "husky": "^4.2.5",
    "internal-ip": "^6.0.0",
    "jest": "^26.0.1",
    "jest-css-modules": "^2.1.0",
    "lint-staged": "^10.2.11",
    "nuxt-svg-loader": "^1.2.0",
    "prettier": "^2.0.5",
    "sanitize.css": "^11.0.1",
    "sass": "^1.26.9",
    "sass-loader": "^8.0.2",
    "stylelint": "^13.6.1",
    "stylelint-config-prettier": "^8.0.1",
    "stylelint-config-standard": "^20.0.0",
    "svg-inline-loader": "^0.8.2",
    "ts-jest": "^26.1.0",
    "vee-validate": "^3.3.5",
    "vue-jest": "^3.0.4"
  }
